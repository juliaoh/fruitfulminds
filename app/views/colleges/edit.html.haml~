- @subtitle = "Edit College"

%span
  = form_for @college_fields, :url => {:action => :update} do |f|
    = render :partial => "college", :locals => {:f => f}
    #submit
      = submit_tag 'Save Changes'


  = button_to 'Delete', colleges_path(@college), :method => :delete, :confirm => "Are you sure you want to delete #{@college_fields.name}?"
